config wifi-device 'radio0'
        option type 'mac80211'
        option hwmode '11g'
        option path 'pci0000:00/0000:00:00.0'
        option htmode 'HT20'
        option channel 'auto'
        option country 'US'

config wifi-iface 'empower0'
        option device 'radio0'
        option mode 'monitor'
        option ifname 'moni0'


