config wifi-device 'radio0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/qca955x_wmac'
        option txpower '20'
        option country 'US'

config wifi-iface 'empower0'
        option device 'radio0'
        option mode 'monitor'
        option ifname 'moni0'

